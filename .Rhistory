boston.spline <- lm(medv ~ bs(lstat, df = 5), data = Boston)
summary(boston.spline)
boston.loess <- loess(medv ~ lstat, data = Boston, span = 0.5)
grid_lstat <- seq(from = min(Boston$lstat), to = max(Boston$lstat), length.out = 100)
boston.fitted <- data.frame(lstat = grid_lstat)
boston.fitted$Linear <- predict(boston.lin, newdata = boston.fitted)
boston.fitted$Log <- predict(boston.log, newdata = boston.fitted)
boston.fitted$Spline <- predict(boston.spline, newdata = boston.fitted)
boston.fitted$Loess <- predict(boston.loess, newdata = boston.fitted)
ggplot(Boston, aes(x = lstat, y = medv)) +
geom_point(alpha = 0.4, color = "grey") +
geom_line(data = boston.fitted, aes(y = Linear, color = "Linear"), size = 1) +
geom_line(data = boston.fitted, aes(y = Log, color = "Log Transformed"), size = 1) +
geom_line(data = boston.fitted, aes(y = Spline, color = "Spline"), size = 1) +
geom_line(data = boston.fitted, aes(y = Loess, color = "Loess"), size = 1) +
labs(title = "Comparison of Regression Models",
x = "Lower Status Population (%)",
y = "Median Value ($1000s)",
color = "Model Type") +
theme_minimal()
